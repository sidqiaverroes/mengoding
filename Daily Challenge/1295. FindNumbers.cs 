//1295. Find Numbers with Even Number of Digits
//30 April 2025

public class Solution {
    public int FindNumbers(int[] nums) {
        int res = 0;
        foreach(var n in nums)
        {
            int x = n;
            int count = 0;
            while(x>0)
            {
                x = x/10;
                count++;
            }
            if(count % 2 == 0) res++;
        }

        return res;
    }
}