//3330. Find the Original Typed String I
//2 Jul 2025

public class Solution
{
    public int PossibleStringCount(string word)
    {
        int count = 0;
        for (int i = 1; i < word.Length; i++)
        {
            if (word[i] == word[i - 1]) count++;
        }

        return count + 1;
    }
}